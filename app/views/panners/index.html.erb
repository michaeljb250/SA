<head>
  <title>Typing Animation</title>
  <link rel="stylesheet">
</head>
<body>
<script src="https://unpkg.com/@hotwired/stimulus@3.0.0/dist/stimulus.umd.js"></script>
 <div data-controller="panner">
     <h1>Tone.js Audio Player</h1>
     <div class="conter">
      <h1 id="text"></h1>
      <div class="button-container">
        <button id="back-btn">Back</button>
        <button id="next-btn">Next</button>
      </div>
      <div id="textt-box">
        <p id="textt"></p>
      </div>
      <h1>Audio Player</h1>
    <button id="start">Start</button>
    <button id="stop">Stop</button>
    <br>
    <label for="panner">Panner:</label>
    <input type="range" id="panner" min="-1" max="1" step="0.1" value="0">
    <br>
</body>
