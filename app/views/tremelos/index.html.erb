<head>
<title>Typing Animation</title>
<link rel="stylesheet">
</head>
<body>
<script src="https://unpkg.com/@hotwired/stimulus@3.0.0/dist/stimulus.umd.js"></script>
<div data-controller="tremelo">
   <h1></h1>
   <div class="conter">
    <h1 id="text"></h1>
    <div class="button-container">
      <button id="back-btn">Back</button>
      <button id="next-btn">Next</button>
    </div>
    <div id="textt-box">
      <p id="textt"></p>
    </div>
    <div class="button-pos">
    <button class="delay-button" id="play">Play</button>
    <button class="delay-button" id="stop">Stop</button>
    <button class="delay-button" id="tremolo">Tremolo On/Off</button>
    </div>
</div>
</body>
